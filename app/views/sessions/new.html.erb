<div class= "row">
<div class= "col-lg-2"></div>
<div class="col-lg-10">
<h1>Sign In</h1>
<div class="col-lg-6 form-line">
<%= form_tag sessions_path do %>
  <div class="form-group">
	<%= label_tag :email %>
  <br>
	<%= email_field_tag :email %>
  </div>
  <div class="form-group">
	<%= label_tag :password %>
  <br>
	<%= password_field_tag :password, '' %>
  </div>
  <div>
	<%= submit_tag 'Sign In' %>
  </div>
</div>
<% end %>
</div>
<div class= "col-lg-2"> </div>
